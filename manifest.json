{
  "manifest_version": 2,

  "name": "Crushit",
  "description": "A notification extension for sabotage",
  "version": "0.5",

  "permissions": [
    "tabs",
    "notifications"
  ],

  "icons": {
  },

  "background": { "scripts": ["assets/js/lodash.core.min.js", "assets/js/jquery-3.1.0.min.js", "background.js"] },

  "browser_action": {
  },

  "content_scripts": [
    {
    "js": ["assets/js/jquery-3.1.0.min.js", "crushit.js"],
    "matches": [ "https://sabotage.internal.mx/"]
    }
  ]
}
